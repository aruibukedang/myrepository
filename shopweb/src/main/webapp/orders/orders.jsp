<head>
<meta charset="utf-8">
<title>订单</title>
<link rel="stylesheet" type="text/css" href="../css/orders.css">
	<link rel="stylesheet" type="text/css" href="../css/header.css"/>
	<link rel="stylesheet" type="text/css" href="../js/layer/mobile/need/layer.css"/>
	<script type="text/javascript" src="../js/layer/layer.js" ></script>
<script src="js/jquery-3.3.1.min.js"></script>
<script type="text/javascript">

function cuifa(id) {
	
	 
	 $.post(
	             "cuifa",
	               {"id":id}
	            	,
	            function(json) {       	            		
	            		 alert("已催促商家发货");
	    	        	if(json.status>0){
	    		        	 location.reload();
	        	
	          }
	         
				},"json");
	 
	 
	             

	 
}


 function sure(id) {
	 
	 if(confirm('确实收货吗?')){
	 
	 $.post(
	             "sureorders",
	               {"id":id}
	            	,
	            function(json) {       	            		
	            		alert("已确认收货");
	    	        	if(json.status>0){
	    		        	 location.reload();
	        	
	          }
	         
				},"json");
	 
	 
	             }
}
 
 function cancle(id) {
	 
	 
	 if(confirm('确实要取消订单吗?')){
		 
		 
		 $.post(
	             "cancle2",
	                {"id":id}
	            	,
	            function(json) {       	            		
	            		alert("已取消");
			        	
			        	 if(json.status>0){
			        	 location.reload();
			        	           }
			                   
	    
	         
				},"json");
		 
	
	 }
	 

	
}

</script>

</head>

<body>
<!---head--------->
	<div class="header">
				
				<jsp:include page="../model/header1.jsp"></jsp:include>
				
		
	</div>
<!---content------->


  

	<div class="title01">
    	<span>商品</span>
        <h1>单价（元）</h1>
        <h2>数量</h2>
        <h3>实付款</h3>
        <h4>交易状态</h4>  
        <p>操作</p>
    </div>
    
    <c:forEach items="${requestScope.orders}" var="r" >
	<!-----01------>
    <div class="title02">
    	<span>订单编号: ${r.code} </span>
        <h1>成交时间：  ${r.date} </h1>
        <a href="product.html"><h2>店铺：</h2></a>  
        
    </div>
    <div class="pic01">
    	<img src="" alt="商品图片">
        <div class="box01">
        	
		
        </div>
        <span>        &nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp</span>
        <p></p>
        <div class="box02">
        <p>共计</p>
        	<h1>￥${r.nowamount}</h1>
            
        </div>
        <div class="box03">
        	 <c:if test="${r.status==0}">
        	<p>待付款</p>
        	</c:if>
        	 <c:if test="${r.status==1}">
        	<p>待发货</p>
        	</c:if>
        	 <c:if test="${r.status==2}">
        	<p>待收货</p>
        	</c:if>
        	 <c:if test="${r.status==3}">
        	<p>已收货</p>
        	</c:if>
        	<c:if test="${r.status==5}">
        	<p>已取消</p>
        	</c:if>
            <a href="#"><h1>订单详情</h1></a>
            <a href="#"><h1>查看物流</h1></a>
        </div>
		  <c:if test="${r.status==0}">
        	<input class="shouhuo" name="" type="button" value="去付款"> 
        	<input class="shouhuo" name="" type="button" onclick="cancle(${r.id});" value="取消订单"> 
        	</c:if>
        	 <c:if test="${r.status==1}">
        	<input class="shouhuo" name="" type="button" onclick="cuifa(${r.id});" value="催发"> 
        	</c:if>
        	 <c:if test="${r.status==2}">
        	<input class="shouhuo" name="" type="button"  onclick="sure(${r.id});" value="确认收货"> 
        	</c:if>
        	 <c:if test="${r.status==3}">
        	<input class="shouhuo" name="" type="button" onclick="退货(${r.id});" value="退货"> 
        	</c:if>
        	<c:if test="${r.status==5}">
        	
        	</c:if> 
    </div>
    
    
    
    
       <c:forEach items="${requestScope.orders_details}" var="t" >
       <c:if test="${t.orders_id==r.id}">
       
    <div class="pic01">
    	<img src="${t.pic}" alt="这是一张图片">
        <div class="box01">
        	<a href="product.html">${t.fullname}</a>
			<p>${t.sale}</p>
        </div>
        <span>￥${t.nowprice}</span>
        <p>${t.count}</p>
        <div class="box02">
        	<h1>￥${t.nowprice*t.count}</h1>
           
        </div>
        <div class="box03">
         
        	
        	
        	
         
        </div>
       
		 
		</div>
		</c:if>
		</c:forEach>
  
</c:forEach>


